<p id="notice"><%= notice %></p>

<h1>Course Orders</h1>

<table>
  <thead>
    <tr>
      <th></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @course_orders.each do |course_order| %>
      <tr>
        <td><%= course_order. %></td>
        <td><%= link_to 'Show', course_order %></td>
        <td><%= link_to 'Edit', edit_course_order_path(course_order) %></td>
        <td><%= link_to 'Destroy', course_order, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

